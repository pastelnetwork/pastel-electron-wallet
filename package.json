{
  "name": "pastelwallet",
  "productName": "Pastel Wallet Fullnode",
  "version": "1.2.0",
  "description": "Pastel Wallet Fullnode (Electron version)",
  "keywords": [
    "Pastel",
    "Wallet"
  ],
  "author": {
    "name": "The Pastel Network",
    "email": "support@pastel.network"
  },
  "license": "MIT",
  "main": ".webpack/main",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "make": "cross-env NODE_ENV=production electron-forge make",
    "test": "jest",
    "format": "eslint --ignore-path=.gitignore --ext .ts,.tsx --fix . && prettier --ignore-path=.gitignore --loglevel=error --write .",
