    "check-prettier": "prettier --ignore-path=.gitignore --list-different .",
    "check-typescript": "tsc",
    "check-type-coverage": "type-coverage --strict --ignoreCatch --atLeast=100",
    "check-all": "yarn check-prettier && yarn check-eslint && yarn check-typescript",
    "postinstall": "husky install"
